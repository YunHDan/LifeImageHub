{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "lifeimagehub",
  "type": "javascript",
  "compatibility_date": "2026-01-13",
  "pages_build_output_dir": ".next",
  "build": {
    "command": "npx next build",
    "cwd": "./"
  },
  "main": ".next/server/app.js",
  "site": {
    "bucket": ".next/static"
  },
  "env": {
    "production": {
      "name": "lifeimagehub-production"
    }
  }
}
